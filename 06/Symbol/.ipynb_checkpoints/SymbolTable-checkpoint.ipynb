    "class SymbolTable:\n",
    "    table = {}\n",
    "    \n",
    "    def addEntry(self, symbol, address):\n",
    "        table[symbol] = address\n",
    "    \n",
    "    def contains(self, symbol):\n",
    "        return symbol in table\n",
    "    \n",
    "    def GetAddress(self, symbol):\n",
    "        return table[symbol]"
